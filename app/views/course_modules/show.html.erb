<p style="color: green"><%= notice %></p>

<%= render @course_module %>

<div>
  <% if can? :update, @course_module %>
    <%= link_to "Edit this course module", edit_course_module_path(@course_module) %> |
  <%end%>
  <%= link_to "Back to course modules", course_modules_path %>

  <% if can? :destroy, @course_module %>
    <%= button_to "Destroy this course module", @course_module, method: :delete %>
  <%end%>
</div>
